{
  "info": {
    "name": "Project Group Beta API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Get Cart Items",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer <JWT_TOKEN>",
            "type": "text"
          }
        ],
        "url": {
          "raw": "http://localhost:3000/cart/",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3000",
          "path": ["cart", ""]
        }
      }
    },
    {
      "name": "Add Cart Item",
      "request": {
        "method": "PUT",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer <JWT_TOKEN>",
            "type": "text"
          }
        ],
        "url": {
          "raw": "http://localhost:3000/cart/add/:product_id",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3000",
          "path": ["cart", "add", ":product_id"]
        }
      }
    },
    {
      "name": "Remove Cart Item",
      "request": {
        "method": "DELETE",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer <JWT_TOKEN>",
            "type": "text"
          }
        ],
        "url": {
          "raw": "http://localhost:3000/cart/remove/:product_id",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3000",
          "path": ["cart", "remove", ":product_id"]
        }
      }
    },
    {
      "name": "Get Products",
      "request": {
        "method": "GET",
        "url": {
          "raw": "http://localhost:3000/products/",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3000",
          "path": ["products", ""]
        }
      }
    },
    {
      "name": "Get Orders",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer <JWT_TOKEN>",
            "type": "text"
          }
        ],
        "url": {
          "raw": "http://localhost:3000/orders/",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3000",
          "path": ["orders", ""]
        }
      }
    }
  ]
}